	,"queries":{
                        "generic":"SELECT canonicalAc, geneName, refseqAc, peptideId FROM biomuta_protein WHERE (lower(geneName) LIKE 'QVALUE' OR lower(canonicalAc) LIKE 'QVALUE' OR lower(refseqAc) LIKE 'QVALUE' OR lower(peptideId) LIKE 'QVALUE') LIMIT 100"

                        ,"query_1":"SELECT canonicalAc, count(distinct posInPep, refResidue, altResidue) FROM biomuta_mutation_eff where refResidue != altResidue AND altResidue != '*' AND canonicalAc IN (CANONS) GROUP BY canonicalAc"
                        ,"query_2":"SELECT A.canonicalAc, count(distinct A.posInPep, A.refResidue, A.altResidue) FROM biomuta_mutation_eff A, biomuta_protein_ann B where A.refResidue != A.altResidue AND A.altResidue != '*' AND A.canonicalAc = B.canonicalAc AND A.posInPep >= B.startPos AND A.posInPep <= B.endPos AND A.canonicalAc IN (CANONS) AND B.annType = 'uniprot' GROUP BY A.canonicalAc"
			,"query_3":"SELECT A.id, A.chr, A.pos, A.ref, A.alt, B.posInpep, B.refResidue, B.altResidue, C.cancerId, '-', CAST(sum(C.frequency) AS UNSIGNED), upper(C.dataSrc) FROM biomuta_mutation A, biomuta_mutation_eff B, biomuta_mutation_freq C  WHERE A.id = B.mutationId AND A.id = C.mutationId AND B.refResidue != B.altResidue AND B.altResidue != '*' AND B.canonicalAc = 'QVALUE' GROUP BY A.id, C.cancerId ORDER BY B.posInpep ASC"


                        ,"query_4":"SELECT B.id, B.doName, sum(A.frequency) FROM biomuta_mutation_freq A, biomuta_cancer B WHERE A.cancerId = B.id AND A.mutationId IN (SELECT DISTINCT mutationId FROM biomuta_mutation_eff WHERE refResidue != altResidue AND altResidue != '*' AND canonicalAc = 'QVALUE') GROUP BY B.doName;"
			
			,"query_5":"SELECT startPos, endPos, ref, alt, annType, annName, annValue FROM biomuta_protein_ann where canonicalAc = 'QVALUE'"
	
			,"query_6":"SELECT DISTINCT A.posInPep, sum(B.frequency) FROM biomuta_mutation_eff A, biomuta_mutation_freq B WHERE A.mutationId = B.mutationId AND A.refResidue != A.altResidue AND A.altResidue != '*' AND A.canonicalAc = 'QVALUE' GROUP BY A.posInPep"
		
			,"query_7":"SELECT mutationId, pmId FROM biomuta_mutation_pmid WHERE mutationId IN (SELECT mutationId FROM biomuta_mutation_eff WHERE canonicalAc = 'QVALUE')" 
			
			,"query_8":"select count(distinct pmId) FROM biomuta_mutation_pmid where mutationId in (SELECT mutationId FROM biomuta_mutation_eff WHERE canonicalAc = 'CANON' )"
		
			,"query_9":"select count(distinct cancerId) FROM biomuta_mutation_freq where mutationId in (SELECT mutationId FROM biomuta_mutation_eff WHERE canonicalAc = 'CANON')"
		

			,"query_10":"SELECT doId, uberonId FROM biomuta_do2uberon"
	}
